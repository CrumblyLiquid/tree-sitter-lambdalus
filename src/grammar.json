{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "lambdalus",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "macro_defs": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "macro_def"
          },
          {
            "type": "STRING",
            "value": ";"
          }
        ]
      }
    },
    "macro_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "macro_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "macro_name": {
      "type": "PATTERN",
      "value": "[A-Z]+"
    },
    "open_paren": {
      "type": "STRING",
      "value": "("
    },
    "close_paren": {
      "type": "STRING",
      "value": ")"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-z]"
    },
    "lambda": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Î»"
        },
        {
          "type": "STRING",
          "value": "\\"
        }
      ]
    },
    "lambda_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_paren"
        },
        {
          "type": "SYMBOL",
          "name": "lambda"
        },
        {
          "type": "FIELD",
          "name": "parameter",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "variable"
            }
          }
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "close_paren"
        }
      ]
    },
    "bracketed": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_paren"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "close_paren"
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "macro_name"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "bracketed"
        },
        {
          "type": "SYMBOL",
          "name": "lambda_expression"
        },
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expression"
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}